{
  "expo": {
    "name": "Self Blog",
    "slug": "my-app",
    "privacy": "public",
    "sdkVersion": "41.0.0", 
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prushakdev.self-blog-rn-app",
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.prushakdev.self_blog_rn_app",
      "versionCode": 2, 
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
